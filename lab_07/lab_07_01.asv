% 1st excercise from Optimalization_problems.pdf
clear all;
close all;

% solving the problem using gp option
cvx_solver sdpt3;
cvx_begin gp
variables  x y
maximize x * y
subject to
    2 * x + y <= 2400
cvx_end

% this problem using logaritms
cvx_begin gp
variables  a b
maximize log(a) + log(b)
subject to
    log(2 * exp(a)) + log(exp(b)) <= log(2400)
cvx_end

% x = exp(a)
% y = exp(b)
